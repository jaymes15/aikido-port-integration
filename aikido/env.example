# Aikido Integration Configuration
# Copy this file to .env and update with your actual values

# Port Ocean Configuration
OCEAN__BASE_URL=http://localhost:8000

# Aikido API Configuration
OCEAN__INTEGRATION__CONFIG__AIKIDO_CLIENT_ID=your_aikido_client_id_here
OCEAN__INTEGRATION__CONFIG__AIKIDO_CLIENT_SECRET=your_aikido_client_secret_here
OCEAN__INTEGRATION__CONFIG__AIKIDO_BASE_URL=https://app.aikido.dev/api/public/v1

# Port API Configuration (if running locally)
PORT_CLIENT_ID=your_port_client_id
PORT_CLIENT_SECRET=your_port_client_secret
PORT_BASE_URL=https://api.getport.io

# Optional: Logging Configuration
LOG_LEVEL=INFO 